<Application x:Class="ADIN.WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:telerik="clr-namespace:System;assembly=mscorlib"
             xmlns:local="clr-namespace:ADIN.WPF">
    <Application.Resources>
        <ResourceDictionary>
            <telerik:String x:Key="Telerik.Windows.Controls.Key">Analog Devices ADIN1300-Eval</telerik:String>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/WhiteTheme.xaml"/>
                <ResourceDictionary Source="Themes/ButtonStyle.xaml"/>
                <ResourceDictionary Source="Themes/TextBlockStyle.xaml"/>
                <ResourceDictionary Source="Themes/CustomControlStyle/GridSplitterStyle.xaml"/>
                <ResourceDictionary Source="Themes/TabControlStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
