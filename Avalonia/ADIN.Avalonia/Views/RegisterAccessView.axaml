<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="ADIN.Avalonia.Views.RegisterAccessView">
	<Grid>
		<HmcCard Density="High" Margin="16">
			<HmcDataGrid>
				<DataGrid.Columns>
					<DataGridTextColumn Binding="{}" Header="Address"/>
					<DataGridTextColumn Binding="{}" Header="Name"/>
					<DataGridTextColumn Binding="{}" Header="Value"/>
					<DataGridTextColumn Binding="{}" Header="Access"/>
					<DataGridTextColumn Binding="{}" Header="Group"/>
					<DataGridTextColumn Binding="{}" Header="Documentation"/>
				</DataGrid.Columns>
			</HmcDataGrid>
		</HmcCard>
	</Grid>
</UserControl>
